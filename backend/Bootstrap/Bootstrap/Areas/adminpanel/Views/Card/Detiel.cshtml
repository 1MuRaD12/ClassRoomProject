@model Card

@{
    ViewData["Title"] = "Detiel";
}

<div class="table-responsive pt-3">
        <table class="table table-dark mt-2">
            <thead>
                <tr>
                    <th>
                        Id
                    </th>
                    <th>
                        Image
                    </th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td>
                        @Model.Id
                    </td>
                    <td>
                        <img class="img-fluid" src="~/assets/image/@Model.Image" style="width:200px; height:100px;">
                    </td>
                </tr>
            </tbody>
        </table>
</div>

